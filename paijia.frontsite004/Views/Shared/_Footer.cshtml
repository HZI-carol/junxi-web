<div class="copy-right">@Html.Raw(WebConfigs.Config.common_page_footer)</div>

<!-- sidebar-->
<div class="module-sidebar">
    <div class="qrcode options" title="二维码">
        <i class="iconfont icon-erweima"></i>
        <div class="qrcode-img hover-item">
            <div class="item">
                <img src="/content/images/qrcode.png" /><span class="title">微信客服</span>
            </div>
        </div>
    </div>
    @if (!string.IsNullOrEmpty(WebConfigs.Config.common_page_serviceqq))
    {
        <a class="qq options" href="http://wpa.qq.com/msgrd?v=3&amp;uin=@(WebConfigs.Config.common_page_serviceqq)&amp;site=qq&amp;menu=yes" title="联系qq">
            <i class="iconfont icon-qq"></i>
        </a>
    }
    <a class="telephone options" href="tel:@WebConfigs.Config.common_page_hotline" title="联系电话">
        <i class="iconfont icon-dianhua11"></i>
        <div class="tel-num hover-item"><span class="label">咨询热线</span><span class="num">@WebConfigs.Config.common_page_hotline</span></div>
    </a><a class="go-top options" href="#" title="回到顶部"><i class="iconfont icon-top"></i></a>
</div>